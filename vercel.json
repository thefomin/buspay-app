{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"version": 2,
	"builds": [
		{
			"src": "index.html",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist"
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/"
		}
	],
	"framework": "vite",
	"buildCommand": "vite build",
	"outputDirectory": "dist",
	"cleanUrls": true,
	"trailingSlash": false
}
